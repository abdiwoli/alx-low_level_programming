#include "main.h"
#include <string.h>
#include <stdlib.h>
/**
 * _strpbrk - function that searches strings 
 * @s: the string or source
 * @accept: the string to search
 * Return : returns pointer
 */

char *_strpbrk(char *s, char *accept)
{
	char *p = malloc(sizeof(char) * (strlen(s) + 1));
	char f = malloc(sizeof(char) * (strlen(s) + 1));
	char *cpy = accept;
	unsigned int i;

	while (*s != '\0')
	{
		for (i = 0 ; i < strlen(accept); i++)
		{
			if (*s == *cpy)
			{
				*f = *s;
			}
			if (strlen(f) >= strlen(accept)
			    f = p;
		}
				
	}
	return (p == accept ? (p) : NULL);
}
