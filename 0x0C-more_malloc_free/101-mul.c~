#include "main.h"
#include <stdlib.h>
#include <stdio.h>
/**
 * main - program that multiplies two positive numbers
 * Return: 0 or 1
 */
int main(int argc, char **argv)
{
	int num1;
	int num2;

	if (argc < 2)
		return (98);
	num1 = atoi(argv[1]);
	num2 = atoi(argv[2]);

	printf("%d\n", num1 * num2);
	return (0);
}
